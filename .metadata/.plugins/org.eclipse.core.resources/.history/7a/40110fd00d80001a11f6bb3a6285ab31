#include <stdio.h>
#include <stdlib.h>
#include <stdio_ext.h>

#include "ejrc1_clase6.h"
#include "../commons/utn_commons.h"

#define LENGTH 6



void ejerc1Aclase6(void) {
	int notas[]={1,2,6,6,8};
	printf("=Cantidad notas cargadas: %d=\n", utnGetAmountIntArrChargedValues(notas, LENGTH,0));
}

void clase5B(void){
	int notas[LENGTH];

	utnInitializeIntArr(notas,0,LENGTH);
	utnShowAllIntArrContent(notas, LENGTH);

	utnLoadIntArrByIndx(notas,LENGTH,0);

	utnShowAllIntArrContent(notas, LENGTH);
}

